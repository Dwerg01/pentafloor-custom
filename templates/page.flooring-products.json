    "d380c34d-7ba9-4dd5-9a00-e7e6fa8c65da": {
      "type": "text-with-icons",
      "blocks": {
        "template--16826169622820__d380c34d-7ba9-4dd5-9a00-e7e6fa8c65da-16690343068a5ce732-0": {
          "type": "text_block",
          "settings": {
            "icon": "trophy",
            "title": "PRODUCT CUSTOMISATION",
            "text": "<p><strong>Custom options available <\/strong>from enhancements to running line products to complete custom solutions - our product designers work with you to create the perfect product.<\/p>"
          }
        },
        "template--16826169622820__d380c34d-7ba9-4dd5-9a00-e7e6fa8c65da-16690343068a5ce732-1": {
          "type": "text_block",
          "settings": {
            "icon": "gears",
            "title": "3 TIERS OF SERVICE",
            "text": "<p><strong>2 Weeks<\/strong> from in-country stock<\/p><p><strong>4-8 weeks<\/strong> from U.S & China<\/p><p><strong>10-12<\/strong> weeks made to order<\/p>"
          }
        },
        "template--16826169622820__d380c34d-7ba9-4dd5-9a00-e7e6fa8c65da-16690343068a5ce732-2": {
          "type": "text_block",
          "settings": {
            "icon": "truck",
            "title": "QUICK SHIP OPTIONS",
            "text": "<p>Quick Ship carpet tile products are available in six weeks or less with select styles available for immediate shipment<\/p>"
          }
        },
        "38d9a668-e84c-4032-98e9-47e7128eb943": {
          "type": "text_block",
          "settings": {
            "icon": "store",
            "title": "LOCAL STOCKING",
            "text": "<p><strong>Local Service Provider<\/strong> PentaFloor has up to 5000 square metres in stock onsite warehouses and immediate access to storage facilities <strong>throughout South Africa<\/strong><\/p>"
          }
        }
      },
