    "a5ef322d-fa00-4f06-a207-bd2e3d39758f": {
      "type": "text-columns",
      "blocks": {
        "template--16826169622820__a5ef322d-fa00-4f06-a207-bd2e3d39758f-1669033120fa96395c-0": {
          "type": "text_block",
          "settings": {
            "enable_image": false,
            "image_width": 60,
            "title": "PEOPLE",
            "text": "<p>Be a great place to work and inspire our people to become the best they can be.<\/p><p><\/p>",
            "button_label": "",
            "button_link": ""
          }
        },
        "template--16826169622820__a5ef322d-fa00-4f06-a207-bd2e3d39758f-1669033120fa96395c-1": {
          "type": "text_block",
          "settings": {
            "enable_image": false,
            "image_width": 650,
            "title": "PLANET",
            "text": "<p>Be a responsible part of society by ensuring that our impact on the environment is ensuring sustainable communities.<\/p>",
            "button_label": "",
            "button_link": ""
          }
        },
        "template--16826169622820__a5ef322d-fa00-4f06-a207-bd2e3d39758f-1669033120fa96395c-2": {
          "type": "text_block",
          "settings": {
            "enable_image": false,
            "image_width": 650,
            "title": "PORTFOLIO",
            "text": "<p>To offer the most innovative flooring solutions to support today’s business environment.<\/p>",
            "button_label": "",
            "button_link": ""
          }
        },
        "cfb5a542-3bd5-4570-8748-a288055bcd5f": {
          "type": "text_block",
          "settings": {
            "enable_image": false,
            "image_width": 650,
            "title": "PARTNERS",
            "text": "<p>Create and sustain a network of customers and suppliers to ensure interactive system to create mutually beneficial relationships.<\/p>",
            "button_label": "",
            "button_link": ""
          }
        },
        "0f81c8dd-7914-425b-85e4-e746d4a41f2d": {
          "type": "text_block",
          "settings": {
            "enable_image": false,
            "image_width": 650,
            "title": "PROFIT",
            "text": "<p>Maximize long-term return to all shareholders, while staying mindful of our overall responsibilities.<\/p>",
            "button_label": "",
            "button_link": ""
          }
        },
        "827a296c-31e7-462b-af57-4e24c8b38a46": {
          "type": "text_block",
          "settings": {
            "enable_image": false,
            "image_width": 650,
            "title": "PRODUCTIVITY",
            "text": "<p>Be a highly effective, lean, dynamic and fast-moving organization.<\/p>",
            "button_label": "",
            "button_link": ""
          }
        }
      },
